+ *((Tref+LIMIT(V(t1),-200,999))/fpar29)**fpar10)
+ + (fpar17)*((dRd*0.5*(1+SQRT(1+4*(MAX(V(d2,d1),0)/fpar15)**(LIMIT(2+fpar16*(V(t1)+Tref-fpar29),0,3)))))))}
